  "scripts": {
    "build": "tsc",
    "build:up": "tsc && NODE_ENV=development node dist/src/index.js",
    "start": "NODE_ENV=production node dist/src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec node --loader ts-node/esm src/index.ts",
    "test": "NODE_ENV=test node --max-old-space-size=6144 --experimental-vm-modules ./node_modules/.bin/jest -i --colors --verbose --forceExit",
    "test:agents": "NODE_ENV=test node --max-old-space-size=6144 --experimental-vm-modules ./node_modules/.bin/jest -i --colors --verbose --forceExit --config jest.agent.config.ts",
    "test:agents:development": "NODE_ENV=test node --max-old-space-size=6144 --experimental-vm-modules ./node_modules/.bin/jest -i --colors --verbose --forceExit --config jest.agent.development.config.ts",
    "test:watch": "NODE_ENV=test node --experimental-vm-modules ./node_modules/.bin/jest -i --watchAll",
    "coverage": "NODE_ENV=test node --experimental-vm-modules ./node_modules/.bin/jest -i --coverage",
    "coverage:coveralls": "NODE_ENV=test node --experimental-vm-modules ./node_modules/.bin/jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint:fix": "eslint src/ tests/ --fix",
    "lint": "eslint src/ tests/",
    "lint:file": "eslint",
    "lint:file:fix": "eslint --fix",
    "prettier": "prettier --check **/**/*.ts",
    "prettier:fix": "prettier --write **/**/*.ts",
    "audit:critical": "npx audit-ci --critical",
    "audit:high": "npx audit-ci --high",
    "prepare": "husky install",
    "redteam:event-assistant": "yarn build; NODE_ENV=test npx promptfoo redteam run --config promptfoo/promptfooconfig-event-assistant.yaml",
    "chroma:up": "docker run -p 8000:8000 chromadb/chroma",
    "chroma:load": "NODE_ENV=test node --loader ts-node/esm src/utils/chromaLoadDocs.ts",
    "commit": "cz",
    "openapi-types:generate": "openapi-typescript http://localhost:3000/v1/openapi.json --output src/types/openapi.d.ts && prettier --write src/types/openapi.d.ts"
  },
